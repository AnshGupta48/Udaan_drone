<?xml version="1.0" ?>
<sdf version="1.7">
  <world name="default">
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Spawn your robot -->
    <include>
      <uri>model://my_robot</uri>
      <name>my_robot</name>
      <pose>0 0 0.1 0 0 0</pose>
    </include>

    <!-- Optional: Add static objects or plugins -->
  </world>
</sdf>
