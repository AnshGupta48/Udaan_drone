<?xml version="1.0"?>
<robot name="udaan">

    <material name="grey">
        <color rgba="0.5 0.5 0.5 1" />
    </material>

    <material name="blue">
        <color rgba="0 0 1 1" />
    </material>

    <material name="green">
        <color rgba="0 1 0 1" />
    </material>

    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.15 0.15 0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
        </visual>
        <inertial>
            <mass value="0.750" />
            <inertia ixx="0.0028125" ixy="0.0" ixz="0.0" iyy="0.0028125" iyz="0.0" izz="0.0015625"/>
        </inertial>
        <collision>
            <geometry>
                <box size="0.15 0.15 0.05" />
            </geometry>
        </collision>
    </link>
 <!--top right part-->
    <link name="top_right_arm">
        <visual>
            <geometry>
                <box size="0.225 0.04 0.05" />
            </geometry>
            <origin xyz="0.075 0.075 0" rpy="0 0 0.785" />
            <material name="grey" />
        </visual>
        <inertial>
            <mass value="0.1" />
            <inertia ixx="3.42e-05" ixy="0.0" ixz="0.0" 
             iyy="4.43e-04" iyz="0.0" izz="4.35e-04"/>
        </inertial>
        <collision>
            <geometry>
                <box size="0.225 0.04 0.05" />
            </geometry>
        </collision>
    </link>

    <link name="motor_top_right_arm">
    <visual>
        <geometry>
            <cylinder radius="0.015" length="0.05" />
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <material name="blue" />
    </visual>
    <inertial>
        <mass value="0.05" />
        <inertia ixx="1.32e-05" ixy="0.0" ixz="0.0"
                 iyy="1.32e-05" iyz="0.0" izz="5.63e-06" />
    </inertial>
    <collision>
        <geometry>
            <cylinder radius="0.015" length="0.05" />
        </geometry>
    </collision>
</link>

<!--top left part-->
    <link name="top_left_arm">
        <visual>
            <geometry>
                <box size="0.225 0.04 0.05" />
            </geometry>
            <origin xyz="-0.075 0.075 0" rpy="0 0 -0.785" />
            <material name="grey" />
        </visual>
        <inertial>
            <mass value="0.1" />
            <inertia ixx="3.42e-05" ixy="0.0" ixz="0.0" 
             iyy="4.43e-04" iyz="0.0" izz="4.35e-04"/>
        </inertial>
        <collision>
            <geometry>
                <box size="0.225 0.04 0.05" />
            </geometry>
        </collision>
    </link>

    <link name="motor_top_left_arm">
        <visual>
            <geometry>
                <cylinder radius="0.015" length="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <material name="blue" />
        </visual>
        <inertial>
            <mass value="0.05" />
            <inertia ixx="1.32e-05" ixy="0.0" ixz="0.0"
                 iyy="1.32e-05" iyz="0.0" izz="5.63e-06" />
        </inertial>
        <collision>
            <geometry>
                <cylinder radius="0.015" length="0.05" />
            </geometry>
        </collision>
    </link>

<!--bottom right part-->
    <link name="bottom_right_arm">
        <visual>
            <geometry>
                <box size="0.225 0.04 0.05" />
            </geometry>
            <origin xyz="0.075 -0.075 0" rpy="0 0 -0.785" />
            <material name="grey" />
        </visual>
        <inertial>
            <mass value="0.1" />
            <inertia ixx="3.42e-05" ixy="0.0" ixz="0.0" 
             iyy="4.43e-04" iyz="0.0" izz="4.35e-04"/>
        </inertial>
        <collision>
            <geometry>
                <box size="0.225 0.04 0.05" />
            </geometry>
        </collision>
    </link>

    <link name="motor_bottom_right_arm">
        <visual>
            <geometry>
                <cylinder radius="0.015" length="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <material name="blue" />
        </visual>
        <inertial>
            <mass value="0.05" />
            <inertia ixx="1.32e-05" ixy="0.0" ixz="0.0"
                 iyy="1.32e-05" iyz="0.0" izz="5.63e-06" />
        </inertial>
        <collision>
            <geometry>
                <cylinder radius="0.015" length="0.05" />
            </geometry>
        </collision>
    </link>
<!--bottom left parts-->
    <link name="bottom_left_arm">
        <visual>
            <geometry>
                <box size="0.225 0.04 0.05" />
            </geometry>
            <origin xyz="-0.075 -0.075 0" rpy="0 0 -2.35" />
            <material name="grey" />
        </visual>
        <inertial>
            <mass value="0.1" />
            <inertia ixx="3.42e-05" ixy="0.0" ixz="0.0" 
             iyy="4.43e-04" iyz="0.0" izz="4.35e-04"/>
        </inertial>
        <collision>
            <geometry>
                <box size="0.225 0.04 0.05" />
            </geometry>
        </collision>
    </link>

    <link name="motor_bottom_left_arm">
        <visual>
            <geometry>
                <cylinder radius="0.015" length="0.05" />
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <material name="blue" />
        </visual>
        <inertial>
            <mass value="0.05" />
            <inertia ixx="1.32e-05" ixy="0.0" ixz="0.0"
                 iyy="1.32e-05" iyz="0.0" izz="5.63e-06" />
        </inertial>
        <collision>
            <geometry>
                <cylinder radius="0.015" length="0.05" />
            </geometry>
        </collision>
    </link>

<!--landing gears-->
<!--sensor links-->
    <link name="imu_link">
    <visual>
        <geometry>
            <box size="0.05 0.05 0.01" />
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <material name="green" />
    </visual>
    <inertial>
        <mass value="0.01" />
        <origin xyz="0 0 0" rpy="0 0 0" />
        <inertia ixx="2.17e-06" ixy="0.0" ixz="0.0" 
                 iyy="2.17e-06" iyz="0.0" izz="4.17e-06"/>
    </inertial>
    <collision>
        <geometry>
            <box size="0.05 0.05 0.01" />
        </geometry>
    </collision>
</link>



<!--top right joints-->
    <joint name="base_top_right_arm_joint" type="fixed">
        <parent link="base_link" />
        <child link="top_right_arm" />
        <origin xyz="0.065 0.065 0" rpy="0 0 0" />
    </joint>

    <joint name="top_right_arm_motor_joint" type="continuous">
    <parent link="top_right_arm" />
    <child link="motor_top_right_arm" />
    <origin xyz="0.125 0.125 0" rpy="0 0 0" />
    <axis xyz="0 0 -1" />
    </joint>
<!--top left joints-->
    <joint name="base_top_left_arm_joint" type="fixed">
        <parent link="base_link" />
        <child link="top_left_arm" />
        <origin xyz="-0.065 0.065 0" rpy="0 0 0" />
    </joint>

    <joint name="top_left_arm_motor_joint" type="continuous">
        <parent link="top_left_arm" />
        <child link="motor_top_left_arm" />
        <origin xyz="-0.125 0.125 0" rpy="0 0 0" />
        <axis xyz="0 0 1" />
    </joint>

<!--bottom right joints-->
    <joint name="base_bottom_right_arm_joint" type="fixed">
        <parent link="base_link" />
        <child link="bottom_right_arm" />
        <origin xyz="0.065 -0.065 0" rpy="0 0 0" />
    </joint>

    <joint name="bottom_right_arm_motor_joint" type="continuous">
    <parent link="bottom_right_arm" />
    <child link="motor_bottom_right_arm" />
    <origin xyz="0.125 -0.125 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    </joint>

<!--bottom left joints-->
    <joint name="base_bottom_left_arm_joint" type="fixed">
        <parent link="base_link" />
        <child link="bottom_left_arm" />
        <origin xyz="-0.065 -0.065 0" rpy="0 0 0" />
    </joint>

    <joint name="bottom_left_arm_motor_joint" type="continuous">
        <parent link="bottom_left_arm" />
        <child link="motor_bottom_left_arm" />
        <origin xyz="-0.125 -0.125 0" rpy="0 0 0" />
        <axis xyz="0 0 -1" />
    </joint>

<!--landing joints-->
<!--sensor joints-->
    <joint name="base_imu_joint" type="fixed">
        <parent link="base_link" />
        <child link="imu_link" />
        <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>
<!--sensors-->
<gazebo reference="imu_link">
    <sensor type="imu" name="imu_sensor">
        <always_on>true</always_on>
        <update_rate>100.0</update_rate>
        <imu>
        <angular_velocity>true</angular_velocity>
        <linear_acceleration>true</linear_acceleration>
        </imu>
        <visualize>true</visualize>
        <plugin name="imu_plugin" filename="libgazebo_ros_imu_sensor.so"/>
    </sensor>
</gazebo>



</robot>
